{
    "name": "dbdiff/dbdiff",
    "type": "project",
    "license": "MIT",
    "bin": ["./dbdiff"],
    "autoload": {
        "psr-4": {
            "DBDiff\\": "src/"
        }
    },
    "require": {
        "aura/cli": "^2.2",
        "symfony/yaml": "^6.4",
        "illuminate/database": "^4.2",
        "illuminate/view": "^4.2",
        "illuminate/container": "^4.2",
        "diff/diff": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
      "post-install-cmd": "scripts/post-install.sh",
      "post-update-cmd": "scripts/post-install.sh"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
